button {
  display       : inline-block;
  border-radius : 9%;
  font-size     : 10pt;
  text-align    : center;      /* 文字位置   */
  cursor        : pointer;     /* カーソル   */
  padding       : 5px 10px;   /* 余白       */
  background    : rgba(0, 0, 0, 0.10);     /* 背景色     */
  color         : #ffffff;     /* 文字色     */
  line-height   : 0.3em;         /* 1行の高さ  */
  opacity       : 0.9;         /* 透明度     */
  transition    : .3s;         /* なめらか変化 */
}
