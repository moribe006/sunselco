package model;

import java.io.Serializable;

public class FoodEnemyBean extends Enemy implements Serializable {
	/**
	 *
	 */
	private static final long serialVersionUID = 1L;
	private String name;
	private int hp;
	private int kcal;
	private String dropItem;

	public FoodEnemyBean() {}
	public FoodEnemyBean(String name, int hp, int kcal ,String dropItem) {
		this.name = name;
		this.hp = hp;
		this.kcal = kcal;
		this.dropItem = dropItem;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getHp() {
		return hp;
	}
	public void setHp(int hp) {
		this.hp = hp;
	}
	public int getKcal() {
		return kcal;
	}
	public String getDropItem() {
		return dropItem;
	}

}
